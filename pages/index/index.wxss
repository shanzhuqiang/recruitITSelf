page{
  height: 100%;
}
/* index选择页 */
.basePage{
  height: 100%;
  background: #fff;
}
.basePage .authMask{
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: rgba(0,0,0,0);
  z-index: 99;
  line-height: 100%;
  text-align: center;
}
.basePage .indexWrap{
  height: 100%;
}
.indexWrap .homeTitle{
  text-align: center;
  font-size: 36rpx;
  padding: 20rpx 0;
	font-family: PingFangSC-Regular;
}
.indexWrap .chooseOneWrap{
  text-align: center;
  height: 315rpx;
  padding: 60rpx 110rpx;
}
.indexWrap .chooseOneWrap .chooseOne{
  width: 228rpx;
  height: 315rpx;
  float: left;
}
.indexWrap .chooseOneWrap .homeBtn{
  float: right;
  margin-top: 200rpx;
}
.indexWrap .chooseTwoWrap{
  text-align: center;
  height: 357rpx;
  padding: 100rpx 110rpx 0;
}
.indexWrap .chooseTwoWrap .chooseTwo{
  width: 264rpx;
  height: 357rpx;
  float: left;
}
.indexWrap .chooseTwoWrap .homeBtn{
  float: right;
  margin-top: 0rpx;
}
.indexWrap .chooseTwoWrap .homeBtn2{
  margin-top: 200rpx;
}
.indexWrap .homeBtn{
  width: 200rpx;
  height: 68rpx;
  line-height: 68rpx;
  background-color: #4c85fb;
	box-shadow: 0 4rpx 13rpx 0 rgba(26, 73, 210, 0.34);
	border-radius: 4rpx;
  margin:  auto;
	font-family: PingFangSC-Regular;
	font-size: 28rpx;
  color: #fff;
  text-align: center;
}
/* 不授权弹框 */
.indexWrap .noAuthWrap{
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: rgba(0,0,0,0.4);
  z-index: 9999
}
.indexWrap .noAuthWrap .noAuthBox{
  width: 600rpx;
  background: #fff;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  border-radius: 10rpx;
}
.indexWrap .noAuthWrap .noAuthBox .titleNo{
  text-align: center;
	font-family: PingFangSC-Medium;
  font-size: 32rpx;
  line-height: 32rpx;
  color: #333;
  padding: 30rpx 0;
}
.indexWrap .noAuthWrap .noAuthBox .content{
	font-family: PingFangSC-Regular;
  text-align: center;
  font-size: 26rpx;
  height: 26rpx;
  line-height: 26rpx;
  color: #999;
  padding-bottom: 30rpx;
}
.indexWrap .noAuthWrap .noAuthBox .bottomBtn{
  border-top: 1rpx solid #ddd;
  height: 90rpx;
  line-height: 90rpx;
}
.indexWrap .noAuthWrap .noAuthBox .bottomBtn .leftBtn{
  width: 50%;
  height: 90rpx;
  line-height: 90rpx;
  border-right: 1rpx solid #ddd;
  color: #666;
  box-sizing: border-box;
  text-align: center;
  font-size: 26rpx;
}
.indexWrap .noAuthWrap .noAuthBox .bottomBtn .rightBtn{
  width: 50%;
  height: 90rpx;
  line-height: 90rpx;
  color: #2d8cf0;
  text-align: center;
  font-size: 26rpx;
  background: #fff;
}
/* home页 */
.homeWrap{
  height: 100%;
  position: relative;
}
.homeWrap .getPhoneMask{
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: rgba(0,0,0,0.4);
  z-index: 999
}
/* 导航栏 */
.homeWrap .headRow{
  height: 76rpx;
  line-height: 76rpx;
  padding: 0 30rpx 15rpx;
}
.homeWrap .headRow .addressWrap{
  float: left;
  width: 110rpx;
}
.homeWrap .headRow .addressWrap .address{
	font-family: PingFangSC-Regular;
	font-size: 26rpx;
  color: #333;
  width: 80rpx;
  text-align: right;
  float: left;
  overflow: hidden;
  /* text-overflow:ellipsis; */
  white-space: nowrap;
}
.homeWrap .headRow .addressWrap .xiala{
  width: 15rpx;
  height: 10rpx;
  float: right;
  margin: 35rpx 0 0 10rpx;
}
.homeWrap .headRow .serachIptWrap{
  margin-left: 120rpx;
  background-color: #f4f4f4;
	border-radius: 2px;
  height: 62rpx;
  padding-top: 14rpx;
  padding-left: 80rpx;
  position: relative;
}
.homeWrap .headRow .serachIptWrap .search{
  width: 26rpx;
  height: 26rpx;
  position: absolute;
  left: 37rpx;
  top: 26rpx;
}
.homeWrap .headRow .serachIptWrap .serachIpt{
  font-size: 26rpx;
}
/* banner */
.homeWrap .middleWrap{
  background: linear-gradient(to bottom, #fcfcfc 0%,#fff 100%);
  height: 660rpx;
  padding: 20rpx 30rpx 0;
}
.homeWrap .middleWrap .bannerWrap{
  height: 280rpx;
}
.homeWrap .middleWrap .bannerWrap .bannerImg{
  width: 100%;
  height: 240rpx;
}
.homeWrap .middleWrap .bannerTitle{
  height: 30rpx;
  line-height: 30rpx;
  font-size: 28rpx;
  text-align: center;
  color: #333;
}
/* 4个按钮 */
.homeWrap .middleWrap .btnWrap{
  padding-top: 10rpx;
  height: 130rpx;
}
.homeWrap .middleWrap .btnWrap .colBtn{
  float: left;
  width: 25%;
	font-family: PingFangSC-Regular;
	font-size: 26rpx;
  text-align: center;
  color: #333;
}
.homeWrap .middleWrap .btnWrap .colBtn .imgBox{
  height: 93rpx;
}
.homeWrap .middleWrap .btnWrap .colBtn .imgBox .mingqi{
  width: 107rpx;
  height: 93rpx;
}
.homeWrap .middleWrap .btnWrap .colBtn .imgBox .zhiwei{
  width: 70rpx;
  height: 92rpx;
}
.homeWrap .middleWrap .btnWrap .colBtn .imgBox .xiangmu{
  width: 93rpx;
  height: 85rpx;
}
.homeWrap .middleWrap .btnWrap .colBtn .imgBox .rencai{
  width: 96rpx;
  height: 81rpx;
}
/* 2个推广 */
.homeWrap .tuiguangWrap{
  padding-top: 25rpx;
  height: 172rpx;
}
.homeWrap .tuiguangWrap .tuiguangBtn{
  width: 335rpx;
  height: 172rpx;
  float: left;
}
/* 分割 */
.homeWrap .rowHeight{
  height: 16rpx;
  background: #f8f8f8;
}
/* 切换title */
.homeWrap > .typeTitleWrap > .title{
  height: 88rpx;
  line-height: 88rpx;
  font-size: 24rpx;
  color: #666;
  border-bottom: 1rpx solid #eee;
  padding: 0 30rpx;
}
.homeWrap > .typeTitleWrap .titleSecond{
  border-bottom: 1rpx solid #eee;
  padding: 0 30rpx;
  font-size: 24rpx;
  color: #666;
  height: 88rpx;
  line-height: 88rpx;
}
.homeWrap > .typeTitleWrap .titleSecond text{
  border-left: 6rpx solid #2e76f9;
  padding-left: 20rpx;
}
.homeWrap > .typeTitleWrap .title text{
  display: inline-block;
  height: 88rpx;
}
.homeWrap > .typeTitleWrap .title .active{
  font-size: 26rpx;
  color: #333;
	font-family: PingFangSC-Medium;
  position: relative;
}
.homeWrap > .typeTitleWrap .title .active::after{
  content: "";
  position: absolute;
  width: 40rpx;
  height: 6rpx;
  background: #2e70f9;
  left: 30rpx;
  bottom: 0rpx
}
/* 最新项目 */
.homeWrap .itemWrap{
  background: #fff;
  border-bottom: 16rpx solid #f8f8f8;
  padding: 50rpx 30rpx 70rpx;
  position: relative;
}
.homeWrap .itemWrap .first{
  position: relative;
  height: 40rpx;
}
.homeWrap .itemWrap .first .box{
	width: 68rpx;
	line-height: 30rpx;
	height: 30rpx;
  text-align: center;
	background-image: linear-gradient(-45deg, 
		#4874f7 0%, 
		#1a49d2 100%), 
	linear-gradient(
		#ffffff, 
		#ffffff);
	background-blend-mode: normal, 
		normal;
	border-radius: 2rpx;
	font-family: PingFangSC-Regular;
  color: #fff;
  font-size: 24rpx;
  position: absolute;
  left: 0;
  top: 0rpx;
}
.homeWrap .itemWrap .first .title{
	font-family: PingFangSC-Medium;
	font-size: 32rpx;
	color: #333;
  height: 34rpx;
  line-height: 34rpx;
  position: absolute;
  left: 0;
  top: 0rpx;
  right: 170rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.homeWrap .itemWrap .first .money{
	font-family: PingFangSC-Medium;
	font-size: 32rpx;
	color: #f34848;
  position: absolute;
  top: 0rpx;
  right: 0;
}
.homeWrap .itemWrap .second{
	font-size: 24rpx;
	color: #666;
  padding: 15rpx 0 25rpx;
}
.homeWrap .itemWrap .second text{
	font-family: PingFangSC-Regular;
  padding: 0 20rpx;
}
.homeWrap .itemWrap .second .middleText{
  border-left: 1rpx solid #666;
}
.homeWrap .itemWrap .second .rightText{
  border-left: 1rpx solid #666;
}
.homeWrap .itemWrap .content{
  font-family: PingFangSC-Regular;
	font-size: 26rpx;
	color: #666;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical
}
.homeWrap .itemWrap .peopleNum{
	font-family: PingFangSC-Regular;
	font-size: 22rpx;
	color: #ccc;
  position: absolute;
  right: 30rpx;
  bottom: 20rpx;
}
/* 最新岗位 */
.homeWrap .itemWrap.itemWrapTwo{
  padding-bottom: 30rpx;
}
.homeWrap .itemWrap.itemWrapTwo .first .title{
  left: 0rpx;
}
.homeWrap .itemWrap.itemWrapTwo .picBox{
  height: 80rpx;
  padding-top: 10rpx;
}
.homeWrap .itemWrap.itemWrapTwo .picBox .left{
  float: left;
}
.homeWrap .itemWrap.itemWrapTwo .picBox .left .logoImg{
  height: 80rpx;
  width: 80rpx;
}
.homeWrap .itemWrap.itemWrapTwo .picBox .right{
  margin-left: 100rpx;
}
.homeWrap .itemWrap.itemWrapTwo .picBox .right .logoName{
	font-family: PingFangSC-Regular;
	font-size: 28rpx;
	color: #333;
}
.homeWrap .itemWrap.itemWrapTwo .picBox .right .logoType{
	font-family: PingFangSC-Regular;
	font-size: 24rpx;
	color: #999;
  padding-top: 5rpx;
}
/* 精英简历列表 */
.homeWrap .typeTitleWrap .jinyingItemWrap{
  padding: 0 30rpx;
  border-bottom: 16rpx solid #f8f8f8;
}
.homeWrap .typeTitleWrap .jinyingItemWrap .firstRow{
  padding: 40rpx 0 20rpx;
  border-bottom: 1rpx solid #f0f0f0;
}
.homeWrap .typeTitleWrap .jinyingItemWrap .firstRow .leftImgBox{
  width: 80rpx;
  height: 80rpx;
  float: left;
}
.homeWrap .typeTitleWrap .jinyingItemWrap .firstRow .leftImgBox .leftImg{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.homeWrap .typeTitleWrap .jinyingItemWrap .firstRow .rightBox{
  margin-left: 110rpx;
}
.homeWrap .typeTitleWrap .jinyingItemWrap .firstRow .rightBox .firstTitle .nameText{
	font-family: PingFangSC-Medium;
	font-size: 34rpx;
  height: 34rpx;
  line-height: 34rpx;
	color: #333;
  max-width: 200rpx;
  display: inline-block;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.homeWrap .typeTitleWrap .jinyingItemWrap .firstRow .rightBox .firstTitle .typeName{
	font-family: PingFangSC-Regular;
	font-size: 26rpx;
  height: 26rpx;
  line-height: 26rpx;
	color: #333;
  margin-left: 24rpx;
}
.homeWrap .typeTitleWrap .jinyingItemWrap .firstRow .rightBox .secondTags{
	font-family: PingFangSC-Regular;
	font-size: 24rpx;
  height: 24rpx;
  line-height: 24rpx;
	color: #999;
  padding-top: 20rpx;
}
.homeWrap .typeTitleWrap .jinyingItemWrap .firstRow .rightBox .secondTags text{
  padding: 0 15rpx;
}
.homeWrap .typeTitleWrap .jinyingItemWrap .firstRow .rightBox .secondTags text.middleTag{
  border-left: 1rpx solid #999;
  border-right: 1rpx solid #999;
}
.homeWrap .typeTitleWrap .jinyingItemWrap .firstRow .rightBox .thirdTags text{
	height: 28rpx;
  line-height: 28rpx;
  text-align: center;
	background-color: #e8eeff;
	border-radius: 2rpx;
	font-family: PingFangSC-Regular;
	font-size: 22rpx;
	letter-spacing: 1px;
	color: #7a91d5;
  padding: 0 13rpx;
  margin-right: 20rpx;
}
.homeWrap .typeTitleWrap .jinyingItemWrap .contentWrap{
  padding: 33rpx 0 40rpx;
}
.homeWrap .typeTitleWrap .jinyingItemWrap .contentWrap .content{
	font-family: PingFangSC-Regular;
	font-size: 26rpx;
	line-height: 36rpx;
	color: #999;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}